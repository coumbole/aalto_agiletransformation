@INPROCEEDINGS{ISI:000300259200030,
  author = {Hughes, Robert},
  title = {Observing Dispersed Agile Project Processes},
  booktitle = {PROCEEDINGS OF THE 10TH EUROPEAN CONFERENCE ON RESEARCH METHODOLOGY
	FOR BUSINESS AND MANAGEMENT STUDIES},
  year = {2011},
  editor = {Ashwin, M},
  pages = {249-255},
  note = {10th European Conference on Research Methodology for Business and
	Management Studies, Normandy Business Sch, Caen, FRANCE, JUN 20-21,
	2011},
  abstract = {Agile software development methods have attracted increasing interest
	in recent years. They hold out the promise of making software projects
	more effective by engaging more interactively with the system users
	and by reducing bureaucratic obstacles. Although users should benefit
	from systems more suited to their needs, agile methods make considerable
	demands on users during the development process. Thus agile development
	is often associated with the collocation of developers and users.
	This paper describes a study of a UK-based software development company
	which implemented a software project using the Scrum agile method
	in conjunction with a client based in Australia. The project required
	intensive, structured, communication by telephone conferences and
	email. This offered the opportunity for a researcher to observe the
	communication between the stakeholders as a silent listener at the
	telephone conferences and the recipient of copies of emails. The
	most immediate research question was whether it was possible to apply
	Scrum principles to a project where the client and user groups were
	split not just by oceans but by time zones where the duration of
	the shared working day was just one hour. This question fitted into
	a broader research agenda about the way variants of a core method
	may be created to deal with the demands of particular project contexts.
	As appropriate for a conference on research methods, this paper explains
	the research approach adopted and its practical results. It compares
	observation with other data collections methods, particularly interviewing.
	A key advantage of observation, particularly when the observer is
	a silent and invisible eavesdropper, was found to be that no demands
	were made on the hard-pressed project participants who were the subjects
	of the study. However, the approach had limitations such as the possibility
	of `deaf spots', that is, important communications taking place outside
	the hearing of the observer. With interviewing, the subjects are
	able to present explanations of their behaviour. While these may
	vary in reliability, they may be more accurate than the observer's
	unassisted interpretation of an episode.},
