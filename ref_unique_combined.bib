@INPROCEEDINGS{ISI:000227984100050,
  author = {O'Sullivan, P and Shaye, S and Hyland, M},
  title = {``On demand software engineering a hybrid software process and architecture
	perspective{''}},
  booktitle = {International Conference on Computing, Communications and Control
	Technologies, Vol 6, Post-Conference Issue, Proceedings},
  year = {2004},
  editor = {Savoie, M and Chu, HW and Michael, J and Pace, P},
  pages = {279-287},
  organization = {Univ Texas Austin; Int Inst Informat \& System; IEEE Comp Soc, Venezuela
	Chapter; Inter Amer Org Higher Educ},
  note = {International Conference on Computing, Communications and Control
	Technologies (CCCT 2004), Austin, TX, AUG 14-17, 2004},
  abstract = {The enterprises of today need to respond in real time to whatever
	the day brings - a change in supply or demand, a shift in the expectations
	or requirements of customers, the vagaries of capital markets or
	the aftermath of a natural disaster. This requires an infrastructure
	that is different in concept and capability at a number of fundamental
	levels. Earlier technologies are based on what the technology can
	do, and not what the business needs to do. This fundamental change
	of perspective is what IBM term ``On Demand Computing{''}. To evolve
	and apply an on ``On Demand{''} computing model in software engineering
	a software architect must incorporate an engineering strategy that
	encompasses the underlying development process, methods and tools.
	This underlying strategy is often referred to as a Software Engineering
	Paradigm. Software engineering process and architecture models are
	generally chosen based on the nature of the challenge and the application,
	the methods and tools used, and the control and deliverables that
	are required. As a consequence, the software engineering model typically
	adopted in software development projects has direct implications
	for all aspects of the project's end-to-end life cycle, including
	development, function test, system test engineering, performance
	engineering, product enablement/support incremental maintenance releases,
	and so on. This paper debates the need for software engineering teams
	to consider hybrid process and architecture model decisions in the
	context of motivating an optimal ``on demand{''} enterprise application
	development agenda. Consequently, we assert that evolving an ``on
	demand{''} software engineering paradigm implies new rules for software
	development organizations, where all engineering capabilities will
	need to support integration of business decisions, processes and
	methods. Motivating an ``on demand{''} engineering methodology to
	derive parallel competencies will promote new rules in all aspects
	of engagement and planning, and represents the next phase in which
	software development teams move beyond simply integrating their processes
	to actually being able to sense and respond to fluctuating conditions
	so to provide organization-wide capabilities in an ``on demand{''}
	way. First, an attempt is made to understand the essence and evolution
	of the computing paradigm. Second, the rationale for evolving an
	``on demand{''} enterprise application development engineering model
	is explored. Third, pertinent software engineering architectural
	concerns are assessed, after which we explore key software engineering
	process methodologies that need to be considered prior to planning
	an optimal software engineering process paradigm. Finally, we shall
	explore considerations for evolving a modified software engineering
	agenda in the context of an architecturally biased process model
	framework.},
  db = {wok},
  filterkimi = {exclude},
  filtermaria = {Exclude},
  isbn = {980-6560-17-5},
  owner = {kimi},
  review15 = {exclude},
  reviewer = {kimi, casper},
  timestamp = {2012.12.19},
  unique-id = {ISI:000227984100050}
}

