@INPROCEEDINGS{ISI:000280731500044,
  author = {Tanner, Maureen},
  title = {Software Methods in Global Software Development: the Case of Mauritius
	and South Africa},
  booktitle = {PROCEEDINGS OF THE INTERNATIONAL CONFERENCE ON INFORMATION MANAGEMENT
	AND EVALUATION},
  year = {2010},
  editor = {Brown, I},
  pages = {388-398},
  note = {International Conference on Information Management and Evaluation,
	Univ Cape Town, Cape Town, SOUTH AFRICA, MAR 25-26, 2010},
  abstract = {Global Software Development (GSD) is a growing sector in Mauritius
	and South Africa (SA). With an increasing number of Information Technology
	(IT) investors, both countries are recognised as IT hubs in the Southern
	African region. Furthermore, due to a diverse cultural setting prevailing
	in Mauritius and SA, particular software development methods which
	are in concurrence with those cultures might be employed. It is thus
	useful to explore how existing software development methods, particularly
	the Waterfall and the Rational Unified Process (RUP) might be employed
	in these cultural. The research method adopted for the study was
	qualitative and empirical in nature. Multiple case studies were conducted
	in Mauritius and SA and data was collected through semi-structured
	interviews. The data collection and analysis processes were guided
	by the Grounded Theory techniques due to the limited number of GSD
	studies conducted in the Mauritian and SA contexts. Based on the
	results, the paper posits that companies engaging themselves in GSD
	in Mauritius and SA are adapting software development methods (Waterfall
	and RUP) to fit their cultural needs. In essence, the methods employed
	blend some form of flexibility and rigour to drive GSD projects towards
	success. The software development methods are thus employed to mitigate
	cultural issues. Another major outcome of this study is that GSD
	teams have a direct impact on the effective use of the software development
	methods. For instance, the level of skills of the GSD team members
	impacts on the effective application of the software development
	methods during the GSD projects. Results of this study might thus
	be useful to practitioners wishing to engage in GSD in Mauritius
	and SA, by providing guidance on how best to employ software development
	methods to achieve GSD success.},
