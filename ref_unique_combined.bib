@INPROCEEDINGS{ISI:000075973800115,
  author = {Shoemaker, CM and Bornstein, JA},
  title = {The Demo III UGV program: A testbed for autonomous navigation research},
  booktitle = {JOINT CONFERENCE ON THE SCIENCE AND TECHNOLOGY OF INTELLIGENT SYSTEMS},
  year = {1998},
  pages = {644-651},
  organization = {IEEE, Control Syst Soc; Natl Sci Fdn; USA, Army Res Off},
  note = {Joint Conference on the Science and Technology of Intelligent Systems
	ISIC/CIRA/ISAS, NATL INST STAND \& TECHNOL, GAITHERSBURG, MD, SEP
	14-17, 1998},
  abstract = {DoD's Demo III research program has entered its second year. DoD is
	examining technologies developed and evaluated within Demo III as
	one of a number of approaches which might be used in the future to
	reduce hazards to our military forces while enabling them to more
	rapidly deploy to distant locations and operate with reduced logistics.
	The core of Demo III is the sensor, computing architecture and computing
	hardware-software development which are prerequisites for autonomous
	vehicle perception of and maneuver in the land combat environment.
	This effort integrates leading edge military concepts with advanced
	technology development and integration. The program provides a unique
	opportunity to evaluate a group of technically and conceptually interdependent
	initiatives through an iterative series of constructive wargames,
	virtual simulations and field exercises with troops. This technology
	development style shares many features with simulation based design
	and the spiral development model, particularly the exceptionally
	early involvement of the customer (in this instance the military
	user) in the entire endeavor. During the first year of the program
	the technology team responsible for simulation and modeling, terrain-obstacle
	sensing, and perception, computing, communication, navigation, mission
	package selection and safety/field exercise support was organized,
	initial data collection and evaluation of early sensor/algorithm
	candidates for obstacle detection were completed and an overall system
	control architecture (4D-RCS) consistent with multivehicle maneuver
	and intelligent behaviors was developed. Demo III's technology integration
	contract for four Experimental Unmanned Vehicles was competitively
	awarded in January 98. The integration effort will provide the infrastructure
	required to have troops interact with the technologies through three
	successive development/experimentation cycles. The second year of
	the program will see field evaluations of subsystem technologies,
	completion of preliminary and critical design reviews, and an initial
	troop field exercise. At its conclusion in the summer of 2001 Demo
	III will enable troop experimentation with small, highly agile, unmanned
	vehicles capable of off-road, semi-autonomous operation at speeds
	of up to 32 km/hr during daylight and 16 km/hr at night. These capabilities
	will be integrated with navigation, mission planning and target acquisition
	technologies which will enable a meaningful assessment of the promise
	of autonomous robotic ground vehicles early in the next century.
	Many of the Demo III initiatives will benefit from and/or provide
	technology transfer to other defense and civil sector robotic vehicle
	development programs.},
