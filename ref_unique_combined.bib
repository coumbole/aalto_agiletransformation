}

@INPROCEEDINGS{ISI:000270815100041,
  author = {Vu, John Huan and Frojd, Niklas and Shenkel-Therolf, Clay and Janzen,
	David S.},
  title = {Evaluating Test-Driven Development in an Industry-sponsored Capstone
	Project},
  booktitle = {PROCEEDINGS OF THE 2009 SIXTH INTERNATIONAL CONFERENCE ON INFORMATION
