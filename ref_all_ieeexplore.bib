@INPROCEEDINGS{6337315,
  author = {da Silva Estacio, B.J.},
  title = {Development of a Set of Best Practices for Distributed Pair Programming},
  booktitle = {Global Software Engineering Workshops (ICGSEW), 2012 IEEE Seventh
