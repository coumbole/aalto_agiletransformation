@INPROCEEDINGS{6005517,
  author = {Cowan, C.L.},
  title = {When the VP is a Scrum Master, You Hit the Ground Running},
  booktitle = {Agile Conference (AGILE), 2011},
  year = {2011},
  pages = {279 -283},
  month = {aug.},
  abstract = {Companies adopting Scrum as a software development framework often
