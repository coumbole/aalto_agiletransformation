}

@INPROCEEDINGS{5876129,
  author = {Feng Ji and Sedano, T.},
  title = {Comparing extreme programming and Waterfall project results},
  booktitle = {Software Engineering Education and Training (CSEE T), 2011 24th IEEE-CS
